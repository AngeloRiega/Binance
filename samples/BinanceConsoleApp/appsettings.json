{
  "User": {
    "ApiKey": "<api key>",
    "ApiSecret": "<api secret>"
  },
  "Api": {
    "RecvWindowDefault": 15000,
    "RateLimiterCountDefault": 3,
    "RateLimiterDurationSecondsDefault": 1,
    "TimestampOffsetRefreshPeriodMinutes": 5
  },
  "UserClient": {
    "KeepAliveTimerPeriod": 60000
  },
  "Logging": {
    "Console": {
      "IncludeScopes": false,
      "LogLevel": {
        "Default": "Debug",
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "File": {
      "Path": "BinanceConsoleApp.log.txt",
      "LogLevel": {
        "Default": "Trace"
      }
    }
  }
}
