{
  "User": {
    "ApiKey": "<api key>",
    "ApiSecret": "<api secret>"
  },
  "ApiOptions": {
    "RecvWindowDefault": 15000,
    "RequestRateLimitCount": 1200,
    "RequestRateLimitDurationMinutes": 1,
    "RequestRateLimitBurstCount": 100,
    "RequestRateLimitBurstDurationSeconds": 1,
    "TimestampOffsetRefreshPeriodMinutes": 5
  },
  "UserOptions": {
    "OrderRateLimitCount": 100000,
    "OrderRateLimitDurationDays": 1,
    "OrderRateLimitBurstCount": 10,
    "OrderRateLimitBurstDurationSeconds": 1
  },
  "UserClient": {
    "KeepAliveTimerPeriod": 1800000
  },
  "Logging": {
    "Console": {
      "IncludeScopes": false,
      "LogLevel": {
        "Default": "Information",
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "File": {
      "Path": "BinanceConsoleApp.log.txt",
      "LogLevel": {
        "Default": "Trace"
      }
    }
  }
}
