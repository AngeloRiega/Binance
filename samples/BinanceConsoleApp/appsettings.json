{
  "User": {
    "ApiKey": "<api key>",
    "ApiSecret": "<api secret>"
  },
  "Api": {
    "RecvWindowDefault": 15000,
    "QueryRateLimitCount": 1200,
    "QueryRateLimitDurationMinutes": 1,
    "QueryRateLimitBurstCount": 100,
    "QueryRateLimitBurstDurationSeconds": 1,
    "OrderRateLimitCount": 100000,
    "OrderRateLimitDurationDays": 1,
    "OrderRateLimitBurstCount": 10,
    "OrderRateLimitBurstDurationSeconds": 1,
    "TimestampOffsetRefreshPeriodMinutes": 5
  },
  "UserClient": {
    "KeepAliveTimerPeriod": 60000
  },
  "Logging": {
    "Console": {
      "IncludeScopes": false,
      "LogLevel": {
        "Default": "Information",
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "File": {
      "Path": "BinanceConsoleApp.log.txt",
      "LogLevel": {
        "Default": "Trace"
      }
    }
  }
}
